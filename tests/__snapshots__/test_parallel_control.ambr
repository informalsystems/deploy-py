                "apalache.3.main",
                "apalache.4.main",
                "apalache.5.main",
                "apalache.6.main",
                "apalache.7.main",
                "apalache.8.main",
                "apalache.9.main"
              ],
              [
                "terminate"
              ]
            ]
          ]
        }
      }
    }
  }
